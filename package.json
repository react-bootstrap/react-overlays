{
  "name": "react-overlays",
  "version": "2.0.0-0",
  "description": "Utilities for creating robust overlay components",
  "author": "Jason Quense <monastic.panic@gmail.com>",
  "repository": "react-bootstrap/react-overlays",
  "license": "MIT",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "files": [
    "lib"
  ],
  "keywords": [
    "react-overlays",
    "react-component",
    "react",
    "overlay",
    "react-component",
    "tooltip",
    "bootstrap",
    "popover",
    "modal"
  ],
  "scripts": {
    "bootstrap": "yarn && yarn --cwd www",
    "build": "rimraf lib && yarn build:cjs && yarn build:esm && yarn build:pick",
    "build:cjs": "babel src -d lib/cjs",
    "build:esm": "babel src -d lib/esm --env-name esm",
    "build:pick": "cherry-pick ../src --cjs-dir=cjs --esm-dir=esm --cwd=lib",
    "deploy-docs": "yarn --cwd www build --prefix-paths && gh-pages -d www/public",
    "lint": "eslint www/*.js www/src src test *.js",
    "prepublishOnly": "yarn build",
    "release": "rollout",
    "start": "yarn --cwd www start",
    "tdd": "cross-env NODE_ENV=test karma start",
    "test": "yarn lint && yarn testonly",
    "testonly": "yarn tdd --single-run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "release": {
    "publishDir": "lib",
    "conventionalCommits": false
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "@restart/hooks": "^0.3.12",
    "dom-helpers": "^5.0.1",
    "popper.js": "^1.15.0",
    "prop-types": "^15.7.2",
    "uncontrollable": "^7.0.0",
    "warning": "^4.0.3"
  },
  "peerDependencies": {
    "react": ">=16.3.0",
    "react-dom": ">=16.3.0"
  },
  "devDependencies": {
    "@4c/rollout": "^1.3.6",
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@emotion/core": "^10.0.15",
    "@react-bootstrap/eslint-config": "^1.2.0",
    "babel-eslint": "^10.0.2",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.2.0",
    "cherry-pick": "^0.5.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-mocha": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "gh-pages": "^2.1.0",
    "husky": "^3.0.2",
    "jquery": "^3.4.1",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-coverage": "^2.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon-chai": "^2.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "4.0.2",
    "lint-staged": "^9.2.1",
    "lodash": "^4.17.14",
    "mocha": "^6.1.4",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-live": "^2.1.2",
    "react-transition-group": "^4.2.2",
    "rimraf": "^3.0.0",
    "simulant": "^0.2.2",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "webpack": "^4.39.1",
    "webpack-atoms": "^11.0.4",
    "webpack-cli": "^3.3.6"
  }
}
